	SEARCH_DUNGEON_OPTION_4 = "Así que todavía me recuerdas.",
	SEARCH_DUNGEON_4_1 = "— No hay muchos bastardos como tú por ahí.",
	SEARCH_DUNGEON_OPTION_5 = "¿Supongo que es demasiado tarde para pedir perdón?",
	SEARCH_DUNGEON_4_2 = "— ¿A quién intentas engañar? Ya has mostrado tu verdadero rostro.",
	SEARCH_DUNGEON_OPTION_6 = "Este lugar parece perfecto para ti.",
	SEARCH_DUNGEON_4_3 = "— ...Vete a la mierda.",
	SEARCH_DUNGEON_OPTION_7 = "Queremos saber a dónde escapó la princesa.",
	SEARCH_DUNGEON_OPTION_8 = "Debes saber dónde se ha ido tu amo.",
	SEARCH_DUNGEON_OPTION_9 = "¿No crees que deberías cooperar y mejorar las cosas para ti?",
