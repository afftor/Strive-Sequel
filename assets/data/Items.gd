func _init():
	for i in partmaterials:
		for k in partmaterials[i]:
			materiallist[k].parts[i] = partmaterials[i][k].duplicate()
	for i in materiallist.values():
		if i.has('tier') && i.tier != '':
			materials_by_tiers[i.tier].append(i.code)

