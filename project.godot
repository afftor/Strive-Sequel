[application]

config/name="Strive for Power 2"
run/main_scene="res://files/Menu.tscn"
config/use_custom_user_dir=true
boot_splash/image="res://assets/images/backgrounds/loading.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://assets/images/gui/timesphere.png"

[autoload]

variables="*res://files/variables.gd"
events="*res://assets/data/eventdata.gd"
images="*res://files/scripts/ResourceImages.gd"
audio="*res://files/scripts/ResourceAudio.gd"
Enemydata="*res://assets/data/enemydata.gd"
Skilldata="*res://assets/data/Skills.gd"
Effectdata="*res://assets/data/Effects.gd"
Traitdata="*res://assets/data/Traits.gd"
TownData="*res://files/TownData.gd"
combatantdata="*res://files/CombatantClass.gd"
state="*res://src/gamestate.gd"
globals="*res://files/scripts/globals.gd"
input_handler="*res://files/input_handler.gd"
races="*res://assets/data/races.gd"
Items="*res://assets/data/Items.gd"
world_gen="*res://src/world_gen.gd"

[debug]

settings/fps/force_fps=60
gdscript/warnings/unused_variable=false
gdscript/warnings/unused_class_variable=false
gdscript/warnings/unused_argument=false

[display]

window/size/width=1920
window/size/height=1080
mouse_cursor/custom_image="res://assets/images/gui/universal/cursordefault.png"
window/stretch/mode="2d"
window/stretch/aspect="keep"

